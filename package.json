{
	"name": "ytscc",
	"version": "1.0.0",
	"description": "Cli tool to check youtube stream chat via terminal",
	"main": "dist/index.js",
	"scripts": {
		"start": "node .",
		"fixDependencies": "node ./utils/fixDependencies.js",
		"build": "tsc",
		"build:watch": "tsc -w",
		"format": "npx prettier --write .",
		"lint": "eslint ./src --ext .ts",
		"lint:fix": "eslint ./src --ext .ts --fix"
	},
	"bin": "dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/uwury/ytscc.git"
	},
	"keywords": [
		"cli",
		"youtube",
		"stream"
	],
	"author": "Alexiy Rybin <alexiy.rybin@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/uwury/ytsc/issues"
	},
	"homepage": "https://github.com/uwury/ytsc#readme",
	"dependencies": {
		"inquirer": "^8.2.4",
		"youtube-chat": "^2.1.1"
	},
	"devDependencies": {
		"@types/node": "^17.0.40",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"prettier": "^2.6.2",
		"typescript": "^4.7.3"
	},
	"engines": {
		"node": ">=12"
	}
}
